<h1>Which article would you like to see?</h1>
<dl>
  <% articles.forEach(function (eachArticle) { %>
    <dt><a href="/articles/<%= eachArticle._id %>"><%= eachArticle.title %></a></dt>
    <dd>Category: <%= eachArticle.category %></dd>
    <dd><%= eachArticle.content[0].content %></dd>
  <% }) %>
</dl>
